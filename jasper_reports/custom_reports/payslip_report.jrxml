	<parameter name="IDS" class="java.lang.Object"/>
	<queryString>
		<![CDATA[WITH company_image AS (
SELECT
1 AS company_id,
iat.store_fname
FROM ir_attachment iat

WHERE
	iat.res_id = 1
	AND iat.res_model = 'res.partner'
	AND iat.index_content = 'image'

LIMIT 1
),
recognized_antiquity AS (
SELECT
	employee_id,
	MIN(date_start) AS date_start
FROM hr_contract
WHERE state != 'draft' AND active IS True
GROUP BY employee_id
),
lic_qty AS (
SELECT
    psl.slip_id AS payslip_id,
    SUM(psl.total) FILTER (WHERE sr.code LIKE 'leave_lic_days') AS leave_lic_days,
    SUM(psl.total) FILTER (WHERE sr.code LIKE 'leave_hol_days') AS leave_hol_days
FROM hr_payslip_line psl
JOIN hr_salary_rule sr
	ON sr.id = psl.salary_rule_id
GROUP BY psl.slip_id
)

SELECT
rc.name AS "Company_name",
 hr_payslip.id AS "payslip_id",
 rc.phone,
 rpc.street,
 rpc.vat AS cuit,
 hr_payslip_line.code,
 hr_payslip_line.name AS "payslip_line_concept",
 CASE
 	WHEN hr_payslip_line.code IN ('H0040') THEN lic_qty.leave_lic_days * 8
	WHEN hr_payslip_line.code IN ('H0140') THEN lic_qty.leave_lic_days
	WHEN hr_payslip_line.code IN ('H0065') AND aaa.code IN ('1038') THEN lic_qty.leave_hol_days * 9
	WHEN hr_payslip_line.code IN ('H0065') THEN lic_qty.leave_lic_days * 8
	WHEN hr_payslip_line.code IN ('H0165') THEN lic_qty.leave_lic_days
	ELSE hr_payslip_line.quantity_print
 END AS "quantity",
 CASE WHEN (hr_salary_rule_category.name = 'Remunerativo' )
     THEN (hr_payslip_line.total)
 END HABERES,
 CASE WHEN (hr_salary_rule_category.name = 'Retencion')
     THEN (0 - hr_payslip_line.total)
 END DESCUENTOS,
 CASE WHEN (hr_salary_rule_category.name = 'No remunerativo')
     THEN ABS(hr_payslip_line.total)
 END EXENTOS,
 hr_payslip.total_payment as "salario_neto",
 hr_payslip.amount_text as "total_texto",
 hr_employee.name AS "employee name",
 hr_employee.cuil AS "employee_cuil",
 hr_employee.other_id as "legajo",
 recognized_antiquity.date_start as "inicio_contrat",
 hr_contract.wage AS "contract_salary",
 hr_job.name as "employee_sector",
 hr_contract_type.name as "employee_category",
 res_bank.name as "bank_name",
 hr_payslip_run.accounting_date AS "accounting_payment_date",
 hr_payslip_run.payment_date AS "payment_date",
 hr_payslip.date_from as "period",
 ci.store_fname AS "Company_image",
 icp.value AS "Filestore_path"

 FROM hr_payslip
 LEFT JOIN hr_payslip_line
 	ON hr_payslip_line.slip_id = hr_payslip.id
 LEFT JOIN hr_salary_rule_category
 	ON hr_salary_rule_category.id=hr_payslip_line.category_id
 LEFT JOIN hr_employee
 	ON hr_employee.id = hr_payslip.employee_id
 LEFT JOIN res_company as rc
 	ON rc.id=1
 LEFT JOIN res_partner as rpc
 	ON rc.partner_id = rpc.id
 LEFT JOIN hr_contract
 	ON hr_contract.employee_id = hr_employee.id
LEFT JOIN account_analytic_account aaa
	ON hr_contract.analytic_account_id = aaa.id
JOIN hr_payroll_structure str
	ON hr_contract.struct_id = str.id
LEFT JOIN res_partner_bank
	ON hr_employee.bank_account_id = res_partner_bank.id
LEFT JOIN res_bank
	ON res_partner_bank.bank_id = res_bank.id
 LEFT JOIN hr_payslip_run
 	ON hr_payslip.payslip_run_id = hr_payslip_run.id
 LEFT JOIN hr_job
 	ON hr_job.id = hr_employee.job_id
 LEFT JOIN hr_contract_type
 	ON hr_contract_type.id=hr_contract.type_id


 LEFT JOIN ir_config_parameter icp
 	ON icp.key = 'filestore.path'
 LEFT JOIN company_image ci
 	ON rpc.id = ci.company_id
JOIN recognized_antiquity
	ON recognized_antiquity.employee_id = hr_payslip.employee_id
LEFT JOIN lic_qty
	ON hr_payslip.id = lic_qty.payslip_id


WHERE $X{IN, hr_payslip.id, IDS}

 AND hr_payslip_line.appears_on_payslip=true
 AND hr_payslip_line.code <> 'NET'
 AND hr_contract.state = 'open'
 AND hr_contract.active = true

 ORDER BY hr_payslip.date_from DESC, hr_payslip.id DESC, hr_payslip_line.code ASC]]>
